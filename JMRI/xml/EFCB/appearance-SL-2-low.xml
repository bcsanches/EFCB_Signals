  <authorgroup xmlns="http://docbook.org/ns/docbook">
    <author>
        <personname><firstname>Bruno</firstname><surname>Sanches</surname></personname>
		<email>bcsanches@gmail.com</email>
    </author>       
  </authorgroup>

  <revhistory xmlns="http://docbook.org/ns/docbook">
    <revision>
        <revnumber>1</revnumber>
